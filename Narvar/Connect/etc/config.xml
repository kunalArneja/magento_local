<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <narvar_connect>
            <account>
                <narvar_api_endpoint>https://ws.narvar.qa/api/v1/</narvar_api_endpoint>
            </account>
            <general>
                <enable>0</enable>
            </general>
           	<status_map>
				<fulfilled>FULFILLED</fulfilled>
				<not_shipped>NOT_SHIPPED</not_shipped>
				<shipped>SHIPPED</shipped>
				<cancelled>CANCELLED</cancelled>
				<returned>RETURNED</returned>
				<partial>PARTIAL</partial>
				<ordered>ORDERED</ordered>
			</status_map>
            <return>
                <reason>other</reason>
            </return>
            <custom_attributes>
                <configurable_product_option>2</configurable_product_option>
            </custom_attributes>
            <batch_initial>
                <batch_initial_period>30</batch_initial_period>
            </batch_initial>
        </narvar_connect>
    </default>
</config>