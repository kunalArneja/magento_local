<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="audit_log_clean" instance="Narvar\ConnectEE\Cron\Audit\Clean" method="execute">
            <schedule>0 * * * *</schedule>
        </job>
        <job name="audit_log_bulk_upload" instance="Narvar\ConnectEE\Cron\Audit\Bulk" method="execute">
            <schedule>0 * * * *</schedule>
        </job>
    </group>
</config>